	});
	
	$(".sed-have-box label").click(function(){
		//判断里面的checkbox是否被选中
		var chked = $(this).find("input[type='checkbox']").attr("checked");
		var spanFont = $(this).children("span");
		if(chked){
			//如果被选中
			$(this).addClass("check-box-bg");
			$(spanFont).css("color","#ffffff");
		}else{
			$(this).removeClass("check-box-bg");
			$(spanFont).css("color","#776766");
		}
	});
	$(".have-go-back").click(function(){
		$(".sed-have-box").hide();
		$(".fast-have-box").show();
		$(".fast-have-box").animate({ marginLeft:'0px',opacity:"1"}, 200,function(){
		}); 
		$(".sed-have-box").animate({ marginLeft:'20px',opacity:"0"}, 300,function(){}); 
	});
